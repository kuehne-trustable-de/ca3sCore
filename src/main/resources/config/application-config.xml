        <comment mode="subsection">Section defining SAML server and token processing</comment>
        <saml>
            <activate>
                <value>true</value>
            </activate>
            <idp>
                <value>http://localhost:50080/realms/master</value>
            </idp>
            <sp>
                <value>saml-client</value>
            </sp>

            <roles>
                <user>
                    <value>*</value>
